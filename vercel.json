{
  "redirects": [
    {
      "source": "/",
      "destination": "/onboarding-portal",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/onboarding-portal", "destination": "/index.html" },
    { "source": "/onboarding-portal/", "destination": "/index.html" },
    { "source": "/onboarding-portal/:path*", "destination": "/:path*" }
  ]
}
